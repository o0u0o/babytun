<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.o0u0o.babytun.dao.GoodsDAO">

    <select id="findById" parameterType="long" resultType="com.o0u0o.babytun.entity.Goods">
        SELECT
            *
        FROM
            t_goods
        WHERE
            goods_id = #{value}
    </select>
</mapper>